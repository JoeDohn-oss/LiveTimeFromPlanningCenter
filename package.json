    "postinstall": "set NODE_OPTIONS=--openssl-legacy-provider && npm run build",
    "start": "node server.js"
  },
  "dependencies": {
    "express": "*",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "express": "^4.17.1",
    "serve-static": "^1.14.1",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
